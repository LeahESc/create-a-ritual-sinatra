<h2>Edit Ritual</h2>
<form method="POST" action="/rituals/<%=@ritual.id%>">
    <input id="hidden" type="hidden" name="_method" value="patch">
    <label for="title">Title:</label>
    <input type="text" name="title" value="<%= @ritual.title %>">
    <label for="content">Description:</label>
    <input type="text" name="description" value="<%=@ritual.description%>">
    <input type="submit" value="submit">
</form>